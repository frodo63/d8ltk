    'custom_contacts' => [
      'variables' => [],
    ],
    'custom_requisites' => [
      'variables' => [],
    ],
    'custom_partners' => [
      'variables' => [],
    ],
    'custom_logistics' => [
      'variables' => [],
    ],
    'custom_403' => [
        'variables' => [],
    ],
    'custom_404' => [
        'variables' => [],
    ],
